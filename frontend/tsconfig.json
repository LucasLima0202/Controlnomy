  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true, // Recomendo também adicionar esta opção
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "skipLibCheck": true, 
    "types": ["node"],
    "resolveJsonModule": true
  }
